{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "react/forbid-prop-types": [0],
    "max-len": [0],
    "no-underscore-dangle": [0],
    "no-param-reassign": [0],
    "react/jsx-props-no-spreading": [0]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  }
}